@model InCity.Models.EventListItem

<table width=100% border="0" style="padding: 0px">
    <tr>
        <td style="text-align: center;" width="33%">
            <h4>
                @Model.mTitle
            </h4>
        </td>
        <td>
            <table width=100% border="1" style="padding: 0px;">
                @foreach (var place in Model.mPlaces)
                {
                    <tr>
                        <td style="text-align: center; border-radius: 7px;" width="50%">
                            @Html.ActionLink(place.mTitle, "Show", "Place", new { pid = place.mId }, new { })
                            <br />
                        </td>
                        <td style="text-align: center;" width="50%">
                            Цены
                        </td>
                    </tr>
                }
            </table>
        </td>
    </tr>
</table>