@model IEnumerable<InCity.Models.PlaceModel>

@{
    ViewBag.Title = "Список событий";
}

<h2>Список событий</h2>

<p>
    @Html.ActionLink("Создать", "NewPlace", new { pPass = "Amaterassu11071959" })
</p>
<table class="table">

@foreach (var item in Model) {
    <tr>
        <td>
            @item.mTitle
            [ 
            @Html.ActionLink("Редактировать", "EditPlace", new { pId = item.mId, pPass = "Amaterassu11071959" }) |
            @Html.ActionLink("Просмотр", "Show", "Place", new { pId = item.mId }, null) |
            @Html.ActionLink("Удалить", "RemovePlace", new { pId = item.mId, pPass = "Amaterassu11071959" })
             ]
        </td>
    </tr>
}
</table>

<div>
    @Html.ActionLink("Назад к панели администратора", "Index", new { pPass = "Amaterassu11071959" })
</div>